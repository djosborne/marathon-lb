{
  "id": "calico-1",
  "container": {
    "type": "DOCKER",
    "docker": {
      "network": "BRIDGE",
      "image": "nginx",
      "portMappings": [{"containerPort": 80, "servicePort": 10000}],
      "parameters": [{"key": "net", "value": "databases"}]
    }
  },
  "instances": 1,
  "cpus": 0.1,
  "mem": 65,
  "labels":{
    "HAPROXY_GROUP":"external",
    "HAPROXY_0_IP_PER_CONTAINER": "true",
    "HAPROXY_0_VHOST": "marathon"
  }
}
